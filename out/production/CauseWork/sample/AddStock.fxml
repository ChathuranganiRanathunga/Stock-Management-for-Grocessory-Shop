<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #00CED1;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddStock">
   <children>
      <Button fx:id="btbAddStock" layoutX="615.0" layoutY="97.0" mnemonicParsing="false" onAction="#AddStock" text="Add Stock">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutY="164.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0">
         <image>
            <Image url="@../Img/StockAdd.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="txtQuantity" layoutX="567.0" layoutY="41.0" prefHeight="39.0" prefWidth="154.0" promptText="Quantity">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <ComboBox fx:id="CmbProducts" layoutX="34.0" layoutY="41.0" prefHeight="39.0" prefWidth="280.0" promptText="Products" />
      <ComboBox fx:id="CmbProductIDs" layoutX="338.0" layoutY="41.0" prefHeight="39.0" prefWidth="185.0" promptText="Product ID" />
   </children>
</AnchorPane>
